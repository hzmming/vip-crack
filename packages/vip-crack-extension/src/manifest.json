{
  "name": "Vip Crack",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16-gray.png",
      "32": "icons/32-gray.png",
      "48": "icons/48-gray.png",
      "128": "icons/128-gray.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injects.js", "content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "downloads"
  ]
}
